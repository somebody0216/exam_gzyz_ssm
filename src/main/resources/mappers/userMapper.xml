<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="gzyz.user_mode.dao">

    <insert id="insertUser" parameterType="gzyz.user_mode.pojo.User">
        insert into exam_user(uid,acc,pwd,phone,isdelete) values(#{uid},#{acc},#{pwd},#{phone},0)
    </insert>

    <select id="selectUser" resultType="MySSMPractice_1.MyProject.pojo.User">
        select * from user where isdelete=0
    </select>
</mapper>